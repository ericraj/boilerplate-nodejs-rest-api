{
  "name": "boilerplate-nodejs-rest-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:ericraj/boilerplate-nodejs-rest-api.git",
  "author": "Eric Rajaonarison <eric.raj12@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "clear": "rimraf ./dist",
    "dev": "prisma generate && nodemon --watch",
    "build": "yarn clear && NODE_ENV=production webpack && yarn db:deploy && yarn db:seed",
    "start": "node dist/index.js",
    "deploy": "git push heroku main"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.0",
    "tslib": "^2.4.0"
  },
  "engines": {
    "node": "16",
    "yarn": "^1.22"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "ts-node -r tsconfig-paths/register src/database/prisma/seed/index.ts"
  }
}
